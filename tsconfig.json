{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/src/*": ["src/*"],
      "@/*": ["src/*"]
    }
  },
  "include": ["src", "index.ts", "nativewind-env.d.ts"],
  "exclude": ["node_modules", "components"]
}
